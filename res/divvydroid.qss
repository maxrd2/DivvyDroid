QWidget { background:#171717; color:#a7a7a7; }

QPushButton { background:#212121; border:none; border-radius:4px; padding:6px; }
QPushButton:hover { background:#2f2f2f; }
QPushButton:pressed { background:#353535; }
QDialogButtonBox QPushButton { padding:10px; min-width:60px; }

QGroupBox { border:1px solid #212121; border-radius:3px; padding-top:20px; }
QGroupBox::title { subcontrol-origin:padding; subcontrol-position:top center; margin-top:5px; }
